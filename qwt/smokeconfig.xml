<config>
    <moduleName>qwt</moduleName>
    <parentModules>
        <module>qtcore</module>
        <module>qtgui</module>
    </parentModules>
    <parts>10</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <functions>
        <name>^qwt.*</name>
    </functions>
    <exclude>
        <signature>.*QwtData::operator=\(const QwtData&amp;\).*</signature>
        <signature>.*QwtSpline::d_data</signature>
    </exclude>
    <classList>
        <class>QwtAbstractScaleDraw</class>
        <class>QwtAbstractScale</class>
        <class>QwtAbstractSlider</class>
        <class>QwtAnalogClock</class>
