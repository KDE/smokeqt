<config>
    <moduleName>okular</moduleName>
    <parts>10</parts>
    <parentModules>
        <module>kde</module>
    </parentModules>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <exclude>
        <!-- private stuff -->
        <signature>.*::d</signature>
        <signature>.*::d_ptr</signature>
        <signature>.*::q_ptr</signature>
        <signature>.*::q</signature>
        <signature>.*QDebug.*</signature>
        <signature>.*Q3.*</signature>
    </exclude>
    <functions>
        <!-- everything in that namespace.. -->
        <name>Okular::.*</name>
    </functions>
    <classList>
        <class>Okular::Action</class>
        <class>Okular::Annotation</class>
        <class>Okular::AnnotationObjectRect</class>
        <class>Okular::AnnotationUtils</class>
        <class>Okular::BrowseAction</class>
        <class>Okular::CaretAnnotation</class>
        <class>Okular::ConfigInterface</class>
        <class>Okular::Document</class>
        <class>Okular::DocumentAction</class>
        <class>Okular::DocumentInfo</class>
        <class>Okular::DocumentSynopsis</class>
        <class>Okular::DocumentViewport</class>
        <class>Okular::EmbeddedFile</class>
        <class>Okular::ExecuteAction</class>
        <class>Okular::ExportFormat</class>
        <class>Okular::FileAttachmentAnnotation</class>
        <class>Okular::FilePrinter</class>
        <class>Okular::FontInfo</class>
        <class>Okular::FormField</class>
        <class>Okular::FormFieldButton</class>
        <class>Okular::FormFieldChoice</class>
        <class>Okular::FormFieldText</class>
        <class>Okular::Generator</class>
        <class>Okular::GeomAnnotation</class>
        <class>Okular::GotoAction</class>
        <class>Okular::GuiInterface</class>
        <class>Okular::HighlightAnnotation</class>
        <class>Okular::InkAnnotation</class>
        <class>Okular::LineAnnotation</class>
        <class>Okular::NormalizedPoint</class>
        <class>Okular::NormalizedRect</class>
        <class>Okular::ObjectRect</class>
        <class>Okular::Page</class>
        <class>Okular::PageSize</class>
        <class>Okular::PageTransition</class>
        <class>Okular::PixmapRequest</class>
        <class>Okular::PrintInterface</class>
        <class>Okular::Quad</class>
        <class>Okular::RegularAreaRect</class>
        <class>Okular::ScriptAction</class>
        <class>Okular::Sound</class>
        <class>Okular::SoundAction</class>
        <class>Okular::SoundAnnotation</class>
        <class>Okular::SourceReference</class>
        <class>Okular::SourceRefObjectRect</class>
        <class>Okular::StampAnnotation</class>
        <class>Okular::Style</class>
        <class>Okular::TextAnnotation</class>
        <class>Okular::TextDocumentConverter</class>
        <class>Okular::TextDocumentGenerator</class>
        <class>Okular::TextEntity</class>
        <class>Okular::TextPage</class>
        <class>Okular::Utils</class>
        <class>Okular::VisiblePageRect</class>
        <class>Okular::Window</class>
    </classList>
</config>
